{
  "name": "react-component-toolkit",
  "main": "index.ts",
  "description": "Component Library",
  "version": "0.0.1",
  "types": "index.d.ts",
  "scripts": {
    "ladle:dev": "ladle serve",
    "ladle:prod": "ladle build && ladle preview",
    "build": "npm run clean && npm run rollup",
    "clean": "shx --silent rm -rf dist && shx --silent rm -rf build_artifacts && shx --silent rm -rf src/unittests/coverage  && shx --silent rm -rf src/unittests/__snapshots__",
    "rollup": "rollup -c && shx --silent rm dist/*.d.ts",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "author": "slindsay",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
